  target 'GoodNewsRxMVVMTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GoodNewsRxMVVMUITests' do
    # Pods for testing
    pod 'RxBlocking', '6.1.0'
    pod 'RxTest', '6.1.0'
  end

end
